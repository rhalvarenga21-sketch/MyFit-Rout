{
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/app",
            "destination": "/app.html"
        },
        {
            "source": "/app/(.*)",
            "destination": "/app.html"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ]
}